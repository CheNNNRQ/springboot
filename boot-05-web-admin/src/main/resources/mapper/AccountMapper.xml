<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--接口的全限定名称-->
<mapper namespace="com.diodi.admin.mapper.AccountMapper">
    <!--id 方法名 resultType 返回值类型-->
    <select id="getAccount" resultType="com.diodi.admin.bean.Account">
       SELECT * from ssm.student where id=#{id}
    </select>
</mapper>